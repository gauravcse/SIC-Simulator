.CODE
NULL LDA STRL
NULL SUB SEAL
NULL STA LEN
NULL MOV X,A
NULL CMP ZERO
NULL JLT LOOP
NULL LDA SEAL
NULL LDX ZERO
RECHK LDS STR
NULL CMPS SEARCH
NULL JEQ LOOP1
NULL TIX 
NULL CMP LEN
NULL JLT RECHK
LOOP LDX ZERO
LOOP1 STX RESULT
END
.DATA
STR WORD "SULAGNA"
SEARCH WORD "UL"
STRL BYTE 7H
SEAL BYTE 2H
LEN RESB 1H
ZERO BYTE 0H
RESULT RESB 1H
